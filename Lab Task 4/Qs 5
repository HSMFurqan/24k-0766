#include <stdio.h>
int main(){
    char input;
    printf("Write anything :");
    scanf(" %c",&input);
if (input >= 'a' && input <='z'){
    printf("It is a small alphabet ");
}
else if (input >= 'A' && input <= 'Z'){
    printf("It is a capital alphabet ");}
else if (input >='0' && input <='9'){
    printf("It is a numeric value ");
}
else
printf("It is a special character ");
return 0 }
