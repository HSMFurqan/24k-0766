#include <stdio.h>
int main() {
    float gpa;
    int gpaRange;
    printf("Enter your GPA (0.0 - 4.0): ");
    scanf("%f", &gpa);
    gpaRange = (gpa * 10);
    switch (gpaRange) {
        case 0 ... 9:
            printf("Failed semesterâ€”registration suspended\n");
            break;
        case 10 ... 19:
            printf("On probation for next semester\n");
            break;
        case 20 ... 29:
            printf("");
            break;
        case 30 ... 34:
            printf("Dean's list for semester\n");
            break;
        case 35 ... 40:
            printf("Highest honors for semester\n");
            break;
        default:
            printf("Invalid GPA entered!\n");
    }
    return 0;
}
